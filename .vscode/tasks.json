{
  "tasks": [
    {
      "type": "cppbuild",
      "label": "build_guildgcc_task",
      "command": "make",
      "args": [
        "-f makefile.linux",
        "arm64"
      ],
      "options": {
        "cwd": "${workspaceFolder}/trinkets/buildgcc/"
      },
      "problemMatcher": [
        "$gcc"
      ],
      "group": {
        "kind": "build",
        "isDefault": true
      },
      "detail": "Builds buildgcc for linux arm64."
    },
    {
      "label": "build_buildgcc",
      "command": "${workspaceFolder}/trinkets/buildgcc/build.linux.arm64",
      "options": {
        "cwd": "${workspaceFolder}/trinkets/buildgcc"
      },
      "type": "shell"
    }
  ],
  "version": "2.0.0"
}