LIBSDIR = -L"%BPP_COMPILER%/mingw64/lib"
CINCS = -I"%BPP_COMPILER%/mingw64/include" -I"%BPP_COMPILER%\mingw64\include\c++\12.1.0" -I"%BPP_ROOT%/src/include"
